{
  "Password": {
    "Reset": {
      "ExpirationMinutes": 5,
      "RedirectTo": "http://localhost:4200/account/reset-password",
      "UseResetPasswordPolicyPolicy": true,
      "ResetPasswordPolicyMultiplierMinutes": 1
    },
    "ValidationRules": {
      "SpecialCharacters": "!@#$%^&*()_+{}|:<>?-=[]\\;',./'",
      "MinimumLength": 2,
      "MaximumLength": 100
    }
  }
}